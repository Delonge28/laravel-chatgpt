{
  "name": "delonge28/laravel-chatgpt",
  "description": "A Laravel package for easy integration with OpenAI's ChatGPT API.",
  "keywords": ["laravel", "chatgpt", "openai", "gpt", "api"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pelle Caram√©s",
      "email": "pelleoe@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^8.0|^9.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^9.3",
    "guzzlehttp/guzzle": "^7.0"
  },

  "autoload": {
    "psr-4": {
      "Delonge28\\LaravelChatGPT\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Delonge28\\LaravelChatGPT\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Delonge28\\LaravelChatGPT\\ChatGPTServiceProvider"
      ],
      "aliases": {
        "ChatGPT": "Delonge28\\LaravelChatGPT\\Facades\\ChatGPT"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}